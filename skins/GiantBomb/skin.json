{
    "name": "GiantBomb",
    "author": "Giant Bomb Wiki Street Team",
    "url": "https://www.giantbomb.com/",
    "description": "A custom MediaWiki skin for the Giant Bomb Wiki.",
    "license-name": "MIT",
    "type": "skin",
    "ValidSkinNames": {
      "giantbomb": {
        "class": "SkinGiantBomb",
        "args": [
          {
            "name": "giantbomb",
            "template": "GiantBombTemplate",
            "styles": [
              "skins.giantbomb"
            ]
          }
        ]
      }
    },
    "AutoloadClasses": {
      "SkinGiantBomb": "includes/SkinGiantBomb.php",
      "GiantBombTemplate": "includes/GiantBombTemplate.php"
    },
    "ResourceModules": {
        "skins.giantbomb.vuecomponentexample": {
            "scripts": [
                "resources/components/VueComponentExample.js"
            ]
        },
        "skins.giantbomb.styles": {
            "styles": [
                "resources/css/styles.css"
            ]
        },
        "skins.giantbomb": {
            "packageFiles": [
                "resources/components/index.js"
            ],
            "dependencies": [
                "skins.giantbomb.vuecomponentexample",
                "vue"
            ]
        }
    },
    "ResourceFileModulePaths": {
      "localBasePath": "",
      "remoteSkinPath": "GiantBomb"
    },
    "manifest_version": 2
  } 